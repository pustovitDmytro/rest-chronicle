                    "200": {
                        "description": "Positive: show user profile",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "example": {
                                        "id": 1,
                                        "name": "Leigh",
                                        "email": "helvy0@feedburner.com"
