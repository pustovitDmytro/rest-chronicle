                        "schema": {
                            "type": "string"
                        },
                        "example": "5NM2p40Z8"
                    },
                    {
                        "name": "Content-Type",
                        "in": "header",
                        "schema": {
                            "type": "string"
                        },
                        "example": "application/json"
                    }
                ],
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
